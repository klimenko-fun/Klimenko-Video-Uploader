<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="presetWindow" prefHeight="687.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.github.klimenko.videouploader.windowControllers.PresetsWindowController">
    <ListView fx:id="listPresets" layoutX="14.0" layoutY="389.0" AnchorPane.bottomAnchor="10.0"
              AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="65.0"/>
    <ToolBar fx:id="toolbar" layoutX="10.0" layoutY="6.0" prefHeight="40.0" prefWidth="725.0"
             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <Button fx:id="btn_tips" mnemonicParsing="false" onAction="#onTipsClicked" text="tips" />
      <Button fx:id="btn_localizeCategories" mnemonicParsing="false" onAction="#onLocalizeCategoriesClicked" text="Localize Categories" />
      <Button fx:id="btn_managePlaylists" mnemonicParsing="false" onAction="#onManagePlaylistsClicked" text="Manage Playlists" />
      <TextField fx:id="txt_nameNewPreset" prefHeight="25.0" prefWidth="142.0" promptText="New Preset Name" />
        <Button fx:id="btn_addNewPreset" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#onPresetAddNewClicked" prefHeight="25.0" text="Add New Preset"/>
   </ToolBar>
    <Label fx:id="label_savedPresets" layoutX="92.0" layoutY="104.0" prefHeight="17.0" text="Saved Presets"
           AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="45.0"/>
</AnchorPane>
